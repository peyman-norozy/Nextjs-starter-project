    "react-dom": "^18",
    "react-redux": "^8.1.2",
    "swiper": "^10.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.6",
    "postcss": "^8",
    "tailwindcss": "^3"
  }
}